{
	"name": "cpp-reference",
	"displayName": "Cpp Reference",
	"description": "View CppReference.com in VS Code",
	"version": "0.1.7",
	"publisher": "Guyutongxue",
	"author": {
		"name": "Guyutongxue",
		"url": "https://guyutongxue.github.io",
		"email": "guyutongxue@163.com"
	},
	"engines": {
		"vscode": "^1.48.0"
	},
	"icon": "img/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/Guyutongxue/VSC_CppReference.git"
	},
	"categories": [
		"Other",
		"Programming Languages"
	],
	"keywords": [
		"cpp",
		"c++",
		"cppreference",
		"search",
		"documentation",
		"docs",
		"devdocs",
		"reference"
	],
	"activationEvents": [
		"onCommand:cppref.open",
		"onCommand:cppref.search",
		"onWebviewPanel:docs",
		"onLanguage:cpp"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cppref.open",
				"title": "View for the current word",
				"category": "Cpp Reference"
			},
			{
				"command": "cppref.search",
				"title": "Search manually",
				"category": "Cpp Reference"
			}
		],
		"keybindings": [
			{
				"command": "cppref.open",
				"key": "ctrl+shift+A",
				"mac": "cmd+shift+A",
				"when": "editorTextFocus"
			}
		],
		"configuration": [
			{
				"title": "Cpp Reference",
				"properties": {
					"cppref.lang": {
						"type": "string",
						"title": "Language",
						"enum": [
							"en",
							"zh",
							"ru",
							"ar",
							"cs",
							"de",
							"fr",
							"it",
							"ja",
							"ko",
							"pl",
							"pt",
							"tr",
							"es"
						],
						"default": "en",
						"description": "Choose the language of online version cppreference.com."
					},
					"cppref.useSearch": {
						"type": "boolean",
						"default": true,
						"description": "Use searching on cppreference.com. If you disable this, you will get result by pre-generated symbol list which may be incomplet and incorrekt."
					},
					"cppref.alternative.enabled": {
						"type": "boolean",
						"title": "Enable alternative version",
						"default": false,
						"description": "If you prefer an alternative version of cppreference.com, switch this on."
					},
					"cppref.alternative.url": {
						"type": "string",
						"title": "Alternative URL",
						"description": "Write your alternative cppreference.com base URL here, like `https://guyutongxue.gitee.io/cppref/zh/`.",
						"default": ""
					},
					"cppref.searchEngine": {
						"type": "string",
						"title": "Search engine",
						"description": "Use which search engine when no result matches your query word.",
						"enum": [
							"Google",
							"Bing",
							"DuckDuckGo",
							"Baidu"
						],
						"default": "Bing"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"node-fetch": "^2.6.0",
		"xml2js": "^0.4.23",
		"xmlhttprequest": "^1.8.0"
	},
	"devDependencies": {
		"@types/node-fetch": "^2.5.7",
		"@types/xml2js": "^0.4.5",
		"vscode": "^1.1.29",
		"@types/node": "^10.5.2",
		"tslint": "^5.11.0",
		"typescript": "^2.9.2"
	},
	"__metadata": {
		"id": "aad6f61c-e4ed-4c3f-a823-307be9931d0e",
		"publisherId": "db7b76f4-7a50-4992-a312-7fa93a4ba447",
		"publisherDisplayName": "Guyutongxue",
		"installedTimestamp": 1647577336824
	}
}